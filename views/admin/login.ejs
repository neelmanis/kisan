<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
  <meta name="description" content="Start your development with a Dashboard for Node.js.">
  <meta name="author" content="Creative Tim">
  <title><%= typeof pageTitle !== 'undefined' ? pageTitle : "NEEL APP " %></title>
  <!-- Favicon -->
  <link href="img/brand/favicon.png" rel="icon" type="image/png">
  <!-- Fonts -->
  <link href="<%= nodeSiteUrl %>assets/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
  <link href="<%= nodeSiteUrl %>assets/css/icons.min.css" rel="stylesheet" type="text/css" />
  <link href="<%= nodeSiteUrl %>assets/css/app.min.css" rel="stylesheet" type="text/css" />
  <script>
    let WEB_ROOT = "<%= nodeSiteUrl %>";
</script>
</head>

<body class="account-body accountbg">

  <!-- Log In page -->
  <div class="container">
      <div class="row vh-100 d-flex justify-content-center">
          <div class="col-12 align-self-center">
              <div class="row">
                  <div class="col-lg-5 mx-auto">
                      <div class="card">
                          <div class="card-body p-0 auth-header-box">
                              <div class="text-center p-3">
                                  <h4 class="mt-3 mb-1 fw-semibold text-white font-18">NEEL Admin Panel </h4>
                                  <p class="text-muted  mb-0">Login</p>
                              </div>
                          </div>
                          <div class="card-body p-0">

                              <!-- Tab panes -->
                              <div class="tab-content">
                                  <div class="tab-pane active p-3" id="LogIn_Tab" role="tabpanel">
                                      <form class="form-horizontal auth-form" id="auth-form">
                                          <div class="form-floating mb-3">
                                              <input type="text" class="form-control" name="username" id="username" placeholder="e.g. John Doe"  autocomplete="off" autofocus="off" autosave="off">
                                              <label for="username" data-title="Username" >Username</label>
                                          </div>
                                          <div class="form-floating mb-3">
                                              <input type="password" class="form-control" name="password" id="password" placeholder="e.g. John Doe"  autocomplete="off" autofocus="off" autosave="off">
                                              <label for="password" data-title="Password">Password</label>
                                          </div>
                                          <div class="form-group mt-4 mb-0 row">
                                              <div class="col-12">
                                                  <button class="btn btn-primary w-100 waves-effect waves-light" type="submit">
                                                    Log In <i class="fas fa-sign-in-alt ms-1"></i></button>
                                              </div>
                                              <!--end col-->
                                          </div>
                                          <div class="form-group mt-4 mb-0 row">
                                            <div class="col-12"><a href="/admin/forget" class="btn btn-primary w-100 waves-effect waves-light">Forgot Password</a>
                                            </div>
                                          </div>
                                          <!--end form-group-->
                                      </form>
                                      <!--end form-->
                                  </div>

                              </div>
                          </div>
                      </div>
                      <!--end card-->
                  </div>
                  <!--end col-->
              </div>
              <!--end row-->
          </div>
          <!--end col-->
      </div>
      <!--end row-->
  </div>
  <!--end container-->
  <!-- End Log In page -->

  <script src="<%= nodeSiteUrl %>assets/js/jquery.min.js"></script>
  <script src="<%= nodeSiteUrl %>assets/js/bootstrap.bundle.min.js"></script>
  <script src="<%= nodeSiteUrl %>assets/js/waves.js"></script>
  <script src="<%= nodeSiteUrl %>assets/js/feather.min.js"></script>
  <script src="<%= nodeSiteUrl %>assets/js/simplebar.min.js"></script>
  <script src="<%= nodeSiteUrl %>assets/js/sweetalert.min.js"></script>
  <script src="<%= nodeSiteUrl %>assets/custom-js/ajax-call.js"></script>
  <% if(typeof scriptFile !=="undefined"){ %>
  <script src="<%= nodeSiteUrl %>assets/custom-js/<%= scriptFile %>.js"></script>
  <% } %>
</body>
</html>