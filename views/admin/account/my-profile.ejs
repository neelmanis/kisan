<div class="main-content" id="panel">

<div class="container-fluid mt--6">
<div class="row">
<div class="col-xl-8 order-xl-1">
<div class="card">
<div class="card-header">
<div class="row align-items-center">
<div class="col-8">
<h3 class="mb-0">Edit profile </h3>
</div>

</div>
</div>



<div class="card-body">
<!--<form method="POST" action="/admin/update-profile">-->
<form id="myProfileForm" enctype="multipart/form-data" >
<h6 class="heading-small text-muted mb-4">User information</h6>
<div class="pl-lg-4">
<div class="row">
<div class="col-lg-6">
<div class="form-group">
<label for="username" data-title="Name" >Username</label>
<input type="text" name="username" id="username" class="form-control" placeholder="Username" value="<%= username %>">
</div>
</div>
<div class="col-lg-6">
<div class="form-group">
<label for="email" data-title="Name" >Email</label>
<input type="email" name="email" id="email" class="form-control" value="<%= email %>">
</div>
</div>
</div>


<div class="row">
<div class="col-lg-6">
<div class="form-group">
<label for="name" data-title="Name" >Name</label>
<input type="text" name="name" id="name" class="form-control" placeholder="First name" value="<%= name  %>">

</div>
</div>

<div class="col-lg-6">
<div class="form-group">
<label class="form-control-label" for="input-email">Password</label>
<input type="text" name="password" id="password" class="form-control" value="">
</div>
</div>
</div>
</div>
<div class="text-center">
  <input type="submit" name="submit" value="Submit" class="btn btn-primary my-4">
  <input type="hidden" name="adminId" id="adminId" class="form-control" value="<%= adminId  %>">
  </div>
</form>
</div>
</div>
</div>
</div>
